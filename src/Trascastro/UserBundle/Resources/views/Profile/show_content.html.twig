{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show list-group">
    <ul>
        <li class="list-group-item"><b>{{ 'profile.show.username'|trans }}:</b> {{ user.username }}</li>
        <li class="list-group-item"><b>{{ 'profile.show.email'|trans }}:</b> {{ user.email }}</li>
        <a href="{{ path('app_post_index') }}" class="btn-info btn-sm">Volver</a>
    </ul>

</div>
