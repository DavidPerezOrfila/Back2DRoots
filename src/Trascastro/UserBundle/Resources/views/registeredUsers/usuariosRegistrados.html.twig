{% extends '::base.html.twig' %}
{% block body %}
    <div class="container">
        <div class="col-lg-12 box-form">
            <h2>Usuarios registrados</h2>
            <div class="count">
                <span class="label label-primary">
                   total de personas: {{ users.getTotalItemCount() }}
                </span>
            </div>
            <div class="box-content box-form">

                {% for u in users %}
                    <img src="{{ u.imageFile }}">
                {% endfor %}

            </div>
        </div>
    </div>
{% endblock %}

